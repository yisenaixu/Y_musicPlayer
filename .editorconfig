# http://editorconfig.org
root = true # 在根目录

[*] # 适用于所有文件
charset = utf-8 # 字符集
indent_style = space # 缩进风格
indent_size = 2 # 缩进大小
end_of_line = crlf #行尾使用 LF(换行符) CR(回车符) crlf(回车换行)
trim_trailing_whitespace = true # 去除行尾的空白字符
insert_final_newline =  = true # 文件末尾插入一个空行
quoto_type = single # 引号类型
